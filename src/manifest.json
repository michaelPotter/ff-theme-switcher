{
	"manifest_version": 2,

	"name": "Theme Switcher",
	"description": "Simple utility to switch between light and dark themes",
	"version": "0.0.2",
	"icons": {
		"48": "icon.png",
		"96": "icon.png",
		"192": "icon.png"
	},

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},

	"permissions": [
		"theme"
	]
}
